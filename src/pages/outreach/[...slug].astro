---
import { getCollection } from "astro:content";
import OutreachLayout from "../../layouts/OutreachLayout.astro";

export async function getStaticPaths() {
  const blogEntries = await getCollection("outreach");
  return blogEntries.map((entry) => ({
    params: { slug: entry.id },
    props: { entry },
  }));
}

const { entry } = Astro.props;
---

<OutreachLayout frontmatter={entry.data} />
