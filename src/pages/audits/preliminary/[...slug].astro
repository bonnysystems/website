---
import { getCollection } from "astro:content";
import PreliminaryAuditLayout from "../../../layouts/PreliminaryAuditLayout.astro";

export async function getStaticPaths() {
  const blogEntries = await getCollection("audits");
  return blogEntries.map((entry) => ({
    params: { slug: entry.id },
    props: { entry },
  }));
}

const { entry } = Astro.props;
---

<PreliminaryAuditLayout frontmatter={entry.data} />
